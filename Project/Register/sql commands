1.    ~/workspace/Register/ $ sqlite3 lecture.db
2.    sqlite> CREATE TABLE 'registrants' (
      ...> 'id' INTEGER PRIMARY KEY,
      ...> 'name' VARCHAR(255),
      ...> 'email' VARCHAR(255)
      ...> )
      ...> ;
3.    sqlite> INSERT INTO registrants (name, email) VALUES ('ALICE', 'alice@example.com');
4.    sqlite> SELECT * FROM registrants;
5.    INSERT INTO registrants (name, email) VALUES ('Bob', 'bob@example.com');